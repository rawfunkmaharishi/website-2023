---
const version = "1.9.3";
const cssHash =
  "sha512-KJRB1wUfcipHY35z9dEE+Jqd+pGCuQ2JMZmQPAjwPjXuzz9oL1pZm2cd79vyUgHQxvb9sFQ6f05DIz0IqcG1Jw==";
const jsHash =
  "sha512-Dqm3h1Y4qiHUjbhxTuBGQsza0Tfppn53SHlu/uj1f+RT+xfShfe7r6czRf5r2NmllO2aKx+tYJgoxboOkn1Scg==";

const providersVersion = "1.13.0";
const providersHash =
  "sha512-5EYsvqNbFZ8HX60keFbe56Wr0Mq5J1RrA0KdVcfGDhnjnzIRsDrT/S3cxdzpVN2NGxAB9omgqnlh4/06TvWCMw==";

const cssURL =
  "https://cdnjs.cloudflare.com/ajax/libs/leaflet/" +
  version +
  "/leaflet.min.css";

const jsURL =
  "https://cdnjs.cloudflare.com/ajax/libs/leaflet/" + version + "/leaflet.js";

// https://github.com/leaflet-extras/leaflet-providers
const providersURL =
  "https://cdnjs.cloudflare.com/ajax/libs/leaflet-providers/" +
  providersVersion +
  "/leaflet-providers.min.js";
---

<link
  rel="stylesheet"
  href={cssURL}
  integrity={cssHash}
  crossorigin="anonymous"
/>

<script
  src={jsURL}
  integrity={jsHash}
  crossorigin="anonymous"
  referrerpolicy="no-referrer"
></script>

<script
  src={providersURL}
  integrity={providersHash}
  crossorigin="anonymous"
  referrerpolicy="no-referrer"
></script>

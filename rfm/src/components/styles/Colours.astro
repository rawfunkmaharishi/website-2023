---
import { RGBs } from "../../conf/colours.js";

const colours = Object.assign(
  {},
  ...Object.keys(RGBs).map(function (key) {
    return {
      [key]: "rgb(" + RGBs[key].join(", ") + ")",
    };
  })
);

var c = ""

c += ":root {\n"

for (var key in colours) {
    c += "  --colour-"
    c += key
    c += ": "
    c += colours[key];
    c += ";\n"
}
c += "}\n"

---
<style set:html={c}><style>

---
import Layout from "../layouts/Layout.astro";
import Carousel
 from "../components/Carousel.astro";
import { fetchFromAPI, getJSONLD } from "../lib/tools";

const band = await fetchFromAPI("raw-funk-maharishi")

---

<Layout title="The Band" jsonLD={getJSONLD(band)}>
  <Carousel items={band.member} wrapAround=true />
</Layout>
